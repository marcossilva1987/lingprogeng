#include <stdio.h>
#include <stdlib.h>

int main() {
     //7. Faça um programa que leia um número e indique se o número está compreendido
    //entre 20 e 50 ou não.
    int n;
    printf("digite um numero");
    scanf("%d", &n);
    if(n>=20 && n<=50) {
        printf("o numero %d esta dentro do intervalo",n);
    }else
        printf("o numero %d não esta dentro do intervalo",n);
    
    
    
    
    
    
    
    
    #include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <string.h>

int main() {
    
  
    
    
     //6. Construa um programa que permita armazenar o salário de 20 pessoas. Calcular
    //e armazenar o novo salário sabendo-se que o reajuste foi de 8%. Imprimir uma
    //li]stagemnumerada com o salário e o novo salário. Declare quantos vetores forem
    //necessários.
    
      float sal[5], newsal[5];
      int i;
    for(i=0; i<5; i++){
    printf("digite seu salario");
    scanf("%f", &sal[i]);
    newsal[i] = (sal[i]*0.08)+sal[i];
}
    printf("\n lista dos salarios \n");
    printf("lista numerica \t salario inicial \t salario reajustado \n");
    for(i=0; i<5; i++) {
        printf("%d\t\t", i+1);
        printf("%2.2f\t\t\t", sal[i]);
        printf("%2.2f\n\n", newsal[i]);
        
    }
    
    
    
    return EXIT_SUCCESS;
}
    
    
    return EXIT_SUCCESS;
